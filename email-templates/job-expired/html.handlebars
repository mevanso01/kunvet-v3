                                  <table border="0" cellpadding="0" cellspacing="0">
                                    <tr>
                                      <!--First Button Begin-->
                                      <td>
                                        <table border="0" cellpadding="0" cellspacing="0">
                                          <tr>
                                            <td id="first-button" class="button">
                                              <a title="Renew This Job" href="https://kunvet.com/login?redirect=%2Fmyjobs" target="_blank" rel="noopener" style="display: inline-block; border-radius: 2px; padding: 14px; mso-border-alt: 1px solid #FF6969; font-weight: 700; color: #FFFFFF; text-decoration: none;">
                                              Renew This Job
                                              </a>
                                            </td>
                                          </tr>
                                        </table>
                                      </td>
                                      <!--First Button End-->

                                      <td>
                                        <table border="0" cellpadding="3" cellspacing="0">
