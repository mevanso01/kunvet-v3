    }

    /* Buttons */
    .button {
      border-radius: 2px;
      font-size: 12px;
      font-weight: 700;
      color: #FFFFFF;
    }

    #first-button {
      border: 2px solid #FFFFFF;
    }

    #first-button:hover {
      background-color: #ffc1c8;
    }

    #second-button {
      border: 2px solid #FF6969;
