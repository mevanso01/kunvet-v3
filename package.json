{
  "name": "kunvet",
  "version": "0.0.1",
  "description": "Job search platform",
  "main": "index.js",
  "author": "Kunvet team",
  "license": "SEE LICENSE IN LICENSE.md",
  "private": true,
  "scripts": {
    "start": "npm-run-all --parallel server client",
    "server": "npm-run-all --parallel server:server server:build",
    "server:build": "webpack --watch --progress --config build/webpack.config.server.js",
    "server:server": "nodemon './dist/server/index.js' --watch './dist/server'",
    "client": "webpack-dev-server --hot --watch --progress --open --history-api-fallback --config build/webpack.config.client.js",
    "build": "npm-run-all --parallel build:server build:client",
    "build:server": "webpack -p --progress --config build/webpack.config.server.js",
    "build:client": "webpack -p --progress --config build/webpack.config.client.js",
    "eslint": "eslint --format 'node_modules/eslint-friendly-formatter' server/ client/ common/"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.5",
    "eslint": "^4.5.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.2.0",
    "eslint-plugin-import": "^2.7.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "sass-loader": "^6.0.6",
    "url-loader": "^0.5.9",
    "vue-loader": "^13.0.4",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "apollo-client": "^1.9.1",
    "apollo-server-koa": "^1.1.0",
    "babel-polyfill": "^6.26.0",
    "font-awesome": "^4.7.0",
    "graphql": "^0.10.5",
    "graphql-compose": "^2.3.2",
    "graphql-compose-mongoose": "^1.8.0",
    "graphql-tag": "^2.4.2",
    "kcors": "^2.2.1",
    "koa": "^2.3.0",
    "koa-bodyparser": "^4.2.0",
    "koa-history-api-fallback": "^0.1.3",
    "koa-mount": "^3.0.0",
    "koa-passport": "^4.0.1",
    "koa-router": "^7.2.1",
    "koa-session": "^5.5.0",
    "koa-static": "^4.0.1",
    "mongoose": "^4.11.7",
    "nodemailer": "^4.1.1",
    "sha1": "^1.1.1",
    "simple-node-logger": "^0.93.28",
    "source-map-support": "^0.4.16",
    "vue": "^2.4.4",
    "vue-apollo": "^2.1.0-beta.21",
    "vue-pell-editor": "^0.0.8",
    "vue-resource": "^1.3.4",
    "vue-router": "^2.7.0",
    "vue2-editor": "^2.0.26",
    "vuetify": "^0.15.7",
    "vuex": "^2.4.1",
    "webpack-material-design-icons": "^0.1.0"
  }
}
