{
  "name": "kunvet",
  "version": "0.0.1",
  "description": "Job search platform",
  "main": "index.js",
  "author": "Kunvet team",
  "license": "SEE LICENSE IN LICENSE.md",
  "private": true,
  "scripts": {
    "start": "npm-run-all --parallel server client",
    "server": "cross-env NODE_ENV=development npm-run-all --parallel server:server server:build",
    "server:build": "webpack --watch --progress --config build/webpack.config.server.js",
    "server:server": "nodemon -w 'server-dist/index.js' './server-dist/index.js'",
    "client": "cross-env NODE_ENV=development webpack-dev-server --hot --watch --progress --open --history-api-fallback --config build/webpack.config.client.js",
    "build": "cross-env NODE_ENV=production npm-run-all --parallel build:server build:client",
    "build:server": "webpack -p --progress --config build/webpack.config.server.js",
    "build:client": "webpack -p --progress --config build/webpack.config.client.js",
    "eslint": "eslint --format 'node_modules/eslint-friendly-formatter' server/ client/ common/"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
