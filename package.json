  "scripts": {
    "start": "npm-run-all --parallel server client",
    "server": "npm-run-all --parallel server:server server:build",
    "server:build": "cross-env NODE_ENV=development webpack --watch --progress --config build/webpack.config.server.js --mode development",
    "server:server": "cross-env NODE_ENV=development nodemon -w 'dist/server' 'dist/server/index.js'",
    "client": "cross-env NODE_ENV=development webpack-dev-server --hot --watch --progress --open --history-api-fallback --disable-host-check --config build/webpack.config.client.js --mode development",
    "build": "cross-env NODE_ENV=production TARGET=lambda node -r esm node_modules/.bin/gulp",
    "eslint": "eslint --format 'node_modules/eslint-friendly-formatter' server/ client/ common/",
    "reset-credentials": "cp config/credentials{.example,}.js",
    "show-prod-config": "cross-env NODE_ENV=production node -r esm scripts/show-static-config.js"
  },
